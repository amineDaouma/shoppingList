{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Slots",
  "type": "array",
  "items": {
    "title": "List",
    "type": "object",
    "properties": {
      "id": {
        "type": "number"
      },
      "name": {
        "type": "string"
      },
      "products": {
        "type": "array",
        "items" : {
          "type" : "string"
        }
      }
    },
    "required": ["id", "name", "products"]
  }
}
